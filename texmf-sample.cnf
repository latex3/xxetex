# sample paths for xxelatex, to be added to a local texmf.cnf

TEXINPUTS.xxelatex      = $TEXMFDOTDIR;$TEXMF/tex/{latex,generic,}//
TEXINPUTS.xxelatex-dev  = $TEXMFDOTDIR;$TEXMF/tex/{latex-dev,latex,generic,}//
LUAINPUTS.xxelatex      = $TEXMFDOTDIR;$TEXMF/scripts/{lualatex,$engine,}/{lua,}//;$TEXMF/tex/{lualatex,latex,luatex,generic,}//
LUAINPUTS.xxelatex-dev  = $TEXMFDOTDIR;$TEXMF/scripts/{lualatex,$engine,}/{lua,}//;$TEXMF/tex/{latex-dev,lualatex,latex,luatex,generic,}//
